# Configuration du service API pour les tests
PORT=3001
LOG_LEVEL=error

# Configuration RabbitMQ pour les tests (utilise un mock ou instance de test)
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_HEARTBEAT=60

# Configuration des retries
MAX_RETRIES=2
INITIAL_RETRY_DELAY=100
MAX_RETRY_DELAY=500

# Configuration de sécurité
TLS_ENABLED=false 